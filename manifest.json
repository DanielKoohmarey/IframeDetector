{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Provides iframe information when detected on the current page, and allows them to be deleted.",
   "icons": {
      "128": "icon-128.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "Iframe Detector",
   "short_name": "IframeDetect",
   "browser_action": {
      "default_icon": "icon-32.png",
      "default_title": "Page does not contain an <iframe>",
      "default_popup": "popup.html"
   },
   "permissions": [
    "tabs","http://*/*", "https://*/*" 
   ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "2.0",
   "homepage_url": "https://www.pericror.com"
}